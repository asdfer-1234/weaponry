boot_splash/image="res://asdfer.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://game/stone/stone.png"

[autoload]

Audio="*res://audio/audio.gd"
Palette="*res://graphics/palette.gd"
